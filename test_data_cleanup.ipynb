    "User_2,\t\"Your product is bad\", 1.52\n",
    "User_3, \"Two thumbs down\", 2\n",
    "User_4, \"Works perfectly!!!\", 5\n",
    "User_5, \"Please refund my money\", 1\n",
    "User_6, \"Works fine\", 4\n",
